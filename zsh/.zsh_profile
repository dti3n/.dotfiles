bindkey -s '^f' "~/.local/bin/tmux-sessionizer\n"

catr() {
    tail -n "+$1" $3 | head -n "$(($2 - $1 + 1))"
}

rcatr() {
    tail -n "$1" $2 | tac
}

cat1Line() {
    cat $1 | tr -d "\n"
}
